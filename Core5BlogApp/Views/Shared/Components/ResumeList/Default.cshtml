@using EntityLayer.Concrete
@model List<Resume>

<div class="container">

    <div class="section-title">
        <h2>Özgeçmiş</h2>
        <p>ÖZGEÇMİŞİM :)</p>
    </div>

    <div class="row">
        <div class="col-lg-12">
            @foreach (var item in Model)
            {
                <div class="resume-item">
                    <h4>@item.ResumeHeader</h4>
                    <h5>@item.ResumeDate</h5>
                    <p><em>@item.ResumeAdress </em></p>
                    <p>
                        @Html.Raw(item.ResumeContent)
                    </p>
                </div>
            }
        </div>
    </div>

</div>

